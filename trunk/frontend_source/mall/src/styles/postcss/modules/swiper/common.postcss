.jfk-swiper{
  &__item{
    &-box{
      position: relative;
      width: 100%;
    }
    &-bg{
      position: absolute;
      top: 0;
      left: 0;
      width: 100%;
      height: 100%;
      background: {
        position: center;
        repeat: no-repeat;
        size: cover;
      }
      .jfk-image__lazy{
        width: 100%;
        height:100%;
      }
    }
  }
}