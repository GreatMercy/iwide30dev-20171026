.jfk-page__orderCoupon{
    .orderCoupon__main{
        margin-top: px2rem(80);
        border-radius: 4px;
        padding: px2rem(60) px2rem(40);
    }
    .orderCoupon__detail{
        margin-top: px2rem(70);
    }
    .jfk-menuList{
       padding-top: px2rem(60);
    }
    .coupon-qrcode{
        padding-left: 3.4375rem;
        padding-right: 3.4375rem;
        padding-top: 3.125rem;
        img{
            width: 14rem;
        }
        .content{
           margin-top: 1.5rem;
           margin-bottom: 2.5rem;
           letter-spacing: 2px;
        }
    }
    .trade-photo{
        overflow: auto;
        .tradeBox{
            padding-left: px2rem(40);
            padding-right: px2rem(40);
            .order-timeId{
                padding-top: px2rem(50);
                padding-bottom: px2rem(50);
                .item{
                    color: #808080;
                    line-height: px2rem(40);
                    span{
                        margin-right: px2rem(20);
                        width: px2rem(110);
                        display: inline-block;
                        text-align: justify;
                    }
                }
            }
            .order__gift__info{
                h2{
                    font-size: px2rem(32);
                }
                .price_box{
                    color: #b2945e;
                }
            }
            .product-button{
                width: px2rem(190);
                margin-top: px2rem(80);
            }
            .trade-service{
                margin-top: px2rem(66);
                ul{
                    li{
                        float: left;
                        width: 25%;
                        margin-top: px2rem(52);
                        i{
                            font-size: px2rem(40);
                            color: #808080;
                        }
                        p{
                            margin-top: px2rem(20);
                            letter-spacing: px2rem(2);
                        }
                    }
                }
            }
            .trade-moreinfo{
                margin-top: px2rem(60);
                h3{
                    margin-top: px2rem(66);
                    color: #808080;
                }
                p{
                    margin-top: px2rem(34);
                    line-height: px2rem(56);
                }
            }
        }
        .trade-product{
            margin-top: px2rem(70);
            a{
                display: block;
                padding-top: px2rem(30);
                padding-bottom: px2rem(30);
                i{
                    margin-left: px2rem(10);
                    vertical-align: middle;
                }
            }
        }
    }
    .coupon-deteleOrder{
        padding-top: px2rem(90);
        h3{
            color: #fff;
            margin-bottom: px2rem(140);
        }
        button{
            width: calc( 306/644 *100%);
            height: px2rem(84);
        }
    }
}