.jfk-pages__orderDetail{
    .arrow{
        &:before,&:after{
            background-color: #bfbfbf;
        }
    }
    .orderDetail__giftcontent{
        .giftcontent_title{
            color: #808080;
            &:before,&:after{
                background-color: #363636;
            }
        }
        .giftcontent_fortune{
            background-color: #1f1f1f;
            .forturn__name{
                color: #808080;
                i{
                    color: #b2945e;
                    text-shadow: 0px 0px 15px #b2945e;
                }
            }
            .forturn__value{
                color: #fff;
            }
        }
    }

}