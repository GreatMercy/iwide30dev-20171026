<?php

/**
 * @SWG\Definition(type="object", @SWG\Xml(name="IwideFans"))
 */
class IwideFans
{
    /**
     * @SWG\Property(type="string", description="id")
     * @var string $id
     */
    public $id;

    /**
     * @SWG\Property(type="string", description="openid")
     * @var string $openid
     */
    public $openid;

    /**
     * @SWG\Property(type="string", description="用户昵称")
     * @var string $nickname
     */
    public $nickname;

    /**
     * @SWG\Property(type="string", description="用户的性别，值为1时是男性，值为2时是女性，值为0时是未知")
     * @var string $sex
     */
    public $sex;

    /**
     * @SWG\Property(type="string", description="用户个人资料填写的省份")
     * @var string $province
     */
    public $province;

    /**
     * @SWG\Property(type="string", description="普通用户个人资料填写的城市")
     * @var string $city
     */
    public $city;

    /**
     * @SWG\Property(type="string", description="国家，如中国为CN")
     * @var string $country
     */
    public $country;

    /**
     * @SWG\Property(type="string", description="用户头像，最后一个数值代表正方形头像大小（有0、46、64、96、132数值可选，0代表640*640正方形头像），用户没有头像时该项为空")
     * @var string $headimgurl
     */
    public $headimgurl;

    /**
     * @SWG\Property(type="string", description="用户特权信息，json 数组，如微信沃卡用户为（chinaunicom）")
     * @var string $privilege
     */
    public $privilege;

    /**
     * @SWG\Property(type="string", description="'只有在用户将公众号绑定到微信开放平台帐号后，才会出现该字段")
     * @var string $unionid
     */
    public $unionid;

    /**
     * @SWG\Property(type="string", description="关注时间")
     * @var string $subscribe_time
     */
    public $subscribe_time;

    /**
     * @SWG\Property(type="string", description="公众号内部id")
     * @var string $inter_id
     */
    public $inter_id;
}