<?php

/**
 * @SWG\Definition(type="object", @SWG\Xml(name="SomaUserAddress"))
 */
class SomaUserAddress
{
    /**
     * @SWG\Property(format="int32")
     * @var integer
     */
    public $address_id;

    /**
     * @SWG\Property(example="7")
     * @var integer
     */
    public $province;

    /**
     * @SWG\Property(example="广东")
     * @var string
     */
    public $province_name;

    /**
     * @SWG\Property(example="17")
     * @var integer
     */
    public $city;

    /**
     * @SWG\Property(example="广州",description="城市名")
     * @var string
     */
    public $city_name;

    /**
     * @SWG\Property(example="176",description="地区ID")
     * @var integer
     */
    public $region;

    /**
     * @SWG\Property(example="天河区",description="地区名")
     * @var string
     */
    public $region_name;

    /**
     * @SWG\Property(example="壬丰大厦",description="详细地址")
     * @var string
     */
    public $address;

    /**
     * @var string
     * @SWG\Property(example="张三",description="联系人姓名")
     */
    public $contact;


    /**
     * @var integer
     * @SWG\Property(example="18888888888",description="电话号码")
     */
    public $phone;

    /**
     * @var integer
     * @SWG\Property(enum={1, 2},description="状体,1可用,2不可用")
     */
    public $status;
}