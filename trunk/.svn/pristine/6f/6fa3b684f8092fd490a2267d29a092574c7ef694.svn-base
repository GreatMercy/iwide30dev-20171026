.jfk-input-number {
  &__box {
    border: solid 1px #666;
  }
  .is-sub {
    &:before {
      background-color: #fffefe;
    }
    &.is-disabled {
      &:before {
        background-color: #333;
      }
    }
  }
  .is-add {
    color: #fffefe;
    &.is-disabled {
      color: #333;
    }
  }
  &__input {
    color: #bfbfbf;
  }
}