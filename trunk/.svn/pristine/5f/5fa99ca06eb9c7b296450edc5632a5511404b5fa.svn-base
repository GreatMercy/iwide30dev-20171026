.jfk-pages__comment {
  .jfk-support{
    margin-bottom: px2rem(100);
  }

  .choose_prt_top{
    padding-top: px2rem(70);
    margin-bottom: px2rem(70);
    padding-bottom: px2rem(60);
    &_item1,&_item2,&_item3{
      @mixin ellipsis 1;
      padding:0 px2rem(30);
    }
    
    &_item1{
      
    }
    &_item2{
      margin-top: px2rem(20);
      margin-bottom: px2rem(10);
    }
    &_item3{
      span{
        margin-right: px2rem(50);
      }
    }
  }
  
  .star_container{
    & .star_title{
      height: px2rem(70);
      line-height: px2rem(70);
      text-align: center;
      @mixin one-line horzin {};
        position:relative;
        &::before {
          width: 5%;
          position: absolute;
          bottom: 0;
          left: 46%;
        }
    }
  }

























}











