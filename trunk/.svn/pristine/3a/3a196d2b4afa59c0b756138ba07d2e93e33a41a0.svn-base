.jfk-pages__detail {
  .banners{
    .jfk-swiper__item-box{
      padding-bottom: 100%;
    }
    .swiper-pagination{
      color: #fff;
      text-align: right;
      padding-right: calc(34 / 750 * 100%);
      box-sizing: border-box;
      &-current{
        font-size: calc(38 / 24)em;
      }
    }
  }
  .detail-top{
    position: relative;
    .icons{
      color: #fff;
      position: absolute;
      top: 12px;
      right: 11px;
      text-align: right;
      z-index: 10;
      .jfk-font{
        padding: 5px;
        & + .jfk-font{
          margin-left: 11px;
        }
      }
    }
  }
  /*分销提示*/
  .hotel-staff {
    line-height: calc(39 / 24);
    padding-top: px2rem(18);
    padding-bottom: px2rem(30);
    .cont{
      text-indent: -1.5em;
      padding-left: 1em;
    }
    .number {
      line-height: 1;
      padding: 0 calc(10 / 48)em;
      vertical-align: -.24em;
      color: #fff;
    }
    .notice-icon-1,.notice-icon {
      text-shadow: 0 0 1em currentColor;
    }
    .notice-icon-1 {
      margin-left: -.4em;
      animation-name: staff-notice;
      animation-iteration-count: 3;
      animation-duration: .7s;
      animation-timing-function: ease;
      animation-play-state: running;
    }
    .unit {
      padding-right: .2em;
      color: #fff;
    }
  }
  /*秒杀*/
  .killsec {
    width: calc(595 / 750 * 100%);
    margin: {
      left: auto;
      right: auto;
    };
    position: relative;
    height: px2rem(242);
    .box {
      position: relative;
      z-index: 4;
      padding-left: px2rem(33);
      height: 100%;
      width: calc(490 / 595 * 100%);
    }
    .time {
      padding-bottom: calc(30 / 24)em;
      .tip {
        line-height: calc(34 / 24);
        width: 2em;
        vertical-align: middle;
        padding-right: calc(10 / 24)em;
      }
      .clock{
        vertical-align: middle;
        letter-spacing: -.2em;
        i {
          vertical-align: bottom;
        }
        .num {
          letter-spacing: normal;
          width: 1.3em;
          text-align: center;
        }
        .date {
          width: 1em;
        }
      }
    }
    .process{
      height: px2rem(2);
      padding: 0 px2rem(8);
      .line {
        width: 100%;
        height: 100%;
        position: relative;
      }
    }
    .number {
      padding-top: calc(10 / 24)em;
      span {
        vertical-align: middle;
      }
      .tip {
        text-indent: -1em;
        margin-right: -.8em;
      }
      .total{
        text-indent: -.8em;
      }
    }
    .layer {
      width: calc(407 / 595 * 100%);
      height: 100%;
      position: absolute;
      z-index: 2;
      top: 0;
      left: 0;
      &:after{
        content: '';
        position: absolute;
        height: 100%;
        width: calc(82 / 407 * 100%);
        right: 0;
        top: 0;
        transform: skew(-18deg, 0deg);
        transform-origin: bottom left;
        z-index: 1;
      }
      &:before{
        content: '';
        position: absolute;
        height: 100%;
        width: 100%;
        z-index: 1;
        top: 0;
        left: 0;
        z-index: 2
      }
    }
    .mask {
      position: absolute;
      right: 0;
      top: 0;
      height: 100%;
      width: calc(188 / 595 * 100%);
      z-index: 1;
      overflow: hidden;
      .miao,.sha{
        position: absolute;
        line-height: 1;
      }
      .miao{
        font-size: px2rem(148);
        top: px2rem(10);
        left: px2rem(36);
      }
      .sha {
        font-size: px2rem(125);
        bottom: 0;
        right: px2rem(-35);
      }
    }
  }
  /* 服务 */
  .service {
    padding: px2rem(76) px2rem(80) px2rem(66) px2rem(80);
    text-align: center;
    .label {
      padding-top: px2rem(12);
    }
    .icon .jfk-font{
      font-size: calc(32 / 24)em;
    }
    &-list {
      display: flex;
      justify-content: space-between;
    }
    &-item {
      &:nth-child(n + 5) {
        display: none;
      }
    }
    .more {
      width: calc(40 / 24)em;
      .icon {
        width: 100%;
        position: relative;
        &:before, &:after {
          content: '';
          position: absolute;
          top: 0;
        }
        &:before,&:after, i {
          width: calc(6 / 24)em;
          height: calc(6 / 24)em;
          background-color: currentColor;
          border-radius: 50%;
        }
        i {
          display: block;
          margin: 0 auto;
        }
        &:before {
          left: 0;
        }
        &:after {
          right: 0;
        }
      }
    }
  }
  /*服务弹框*/
  .jfk-popup__service {
    .popup-box {
      padding: px2rem(36) px2rem(60) px2rem(50) px2rem(50);
    }
    .title {
      line-height: px2rem(100);
      text-align: center;
    }
    .popup-service {
      &-item {
        margin-top: px2rem(36);
        position: relative;
        padding-left: px2rem(82);
      }
      .desc {
        line-height: calc(46 / 28);
      }
      .label {
        line-height: calc(54 / 24);
      }
      .icon{
        font-size: calc(36 / 28)em;
        position: absolute;
        top: calc(15 / 36)em;
        left: 0;
      }
    }

  }
  .notice {

  }
  /*规格弹框*/
  .jfk-popup__specTicket {
    bottom: px2rem(130);
    max-height: calc(100% - px2rem(40 + 130));
    .popup-box {
      padding: px2rem(68) px2rem(40) px2rem(40);
    }
    .section-title {
      padding-bottom: px2rem(52);
      line-height: 1;
      @mixin one-line horizotal {
        bottom: 0;
        left: 0;
      }
    }
    .item {
      .title {
        padding-top: px2rem(58);
        padding-bottom: px2rem(6);
      }
    }
    .jfk-radio {
      margin-right: px2rem(20);
      margin-top: px2rem(30);
      &:last-child {
        margin-right: 0;
      }
    }
  }
  /* 规格价格 */
  .good-spec__footer {
    height: px2rem(100);
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    z-index: 100000;
    .price {
      width: calc(450 / 750 * 100%);
      height: px2rem(100);
      box-sizing: border-box;
      padding-left: px2rem(60);
      span {
        display: inline-block;
      }
      .jfk-price__currency{
        vertical-align: .1em;
        margin-right: -.5em;
      }
    }
    .control {
      width: calc(300 / 750 * 100%);
    }
  }
  /*推荐*/
  .recommendation {
    .tip {
      padding: {
        top: px2rem(60);
        bottom: px2rem(47);
      }
    }
  }
  .info {
    padding: {
      left: px2rem(80);
      right: px2rem(80);
    }
    .name {
      line-height: calc(55 / 38);
      @mixin ellipsis 2;
    }
    .sales {
      margin-top: calc(18 / 24)em;
      margin-bottom: calc(10 / 24)em;
      .suppier {
        @mixin one-line {
          height: auto;
          top: 25%;
          bottom: 25%;
          right: 0;
          background-color: currentColor;
        }
        padding-right: calc(28 / 24)em;
        margin-right: calc(18 / 24)em;
      }
      .number {
        padding-left: calc(12 / 24)em;
      }
    }
    .price-tag {
      display: inline-block;
      margin-right: calc(14 / 24)em;
      color: #fff;
      text-shadow: 0px 0px 7px rgba(41, 33, 20, 0.81);
      letter-spacing: -.1em;
      padding: 0 calc(10 / 24)em;
      line-height: calc(36 / 24);
      vertical-align: .2125em;
      position: relative;
      .mask {
        position: absolute;
        top: 0;
        left: 0;
        width: 100%;
        height: 100%;
        background-color: currentColor;
        z-index: -1;
        border-radius: px2rem(var(--border-radius-small));
      }
    }
    .product-price-package,.product-price-market {
      display: inline-block;
    }
    .product-price-market {
      vertical-align: .3em;
    }
    .date-norm{
      padding-top: calc(36 / 24)em;
      padding-bottom: calc(24 / 24)em;
      padding-left: px2rem(70);
      .triangle{
        vertical-align: middle;
        padding-left: calc(5 / 24)em;
      }
    }
  }
  .jfk-support{
    margin-bottom: px2rem(100);
  }
  /*底部*/
  .footer{
    position: fixed;
    bottom: 0;
    left: 0;
    z-index: 1000;
    width: 100%;

    .links{
      width: calc(340 / 750 * 100%);
      .jfk-flex{
        height: px2rem(100);
      }
    }
    .link{
      flex-grow: 1;
      text-align: center;
      text-decoration: none;
      .text{
        line-height: 1;
      }
    }
    .control{
      width: calc(410 / 750 * 100%);
      letter-spacing: .1em;
    }
  }
}
@keyframes staff-notice {
  0% {
    opacity: 0
  }
  100% {
    opacity: 1
  }
}