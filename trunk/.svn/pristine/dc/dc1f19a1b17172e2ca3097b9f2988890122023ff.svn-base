 * @return {Promise} 返回一个promise
 */
const deleteAccount = (data, config, version = 'v1') => {
  let url = apiConfig[version] && apiConfig[version].DELETE_ACCOUNT || apiConfig['v1'].DELETE_ACCOUNT
  return ajax.delete(url, data)
}

