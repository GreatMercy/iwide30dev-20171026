
        .add{background-color: #F6F6F6; width: 110px; height: 40px;color: #B69A6A; border: 1px solid #EFEFEF;float: right;margin: 20px 20px 0 0;border-radius: 5px;}
        /*这个是表格样式*/
        table {table-layout: fixed;empty-cells: show; border-collapse: collapse;margin: 0 auto;text-align:center; }
        td {height: 50px;}
        .t1 { border: 1px solid #cad9ea;color: #666; width: 95%;font-size: 12px;}
        .t1 thead{border-style:none none solid none;border-color: #DAE1E9;}
        .t1 th{height: 45px;color: #333;font-weight: normal;}
        .t1 th:nth-child(1){width:10%;}
        .t1 th:nth-child(2){width:20%;}
        .t1 th:nth-child(3){width:10%;}
        .t1 th:nth-child(4){width:10%;}
        .t1 th:nth-child(5){width:10%;}
        .t1 th:nth-child(6){width:10%;}
        .t1 th:nth-child(7){width:10%;}
        .t1 th:nth-child(8){width:20%;}
        .t1 td, table.t1 th {border: 1px solid #fff; text-align: center;}
        .t1 tr:nth-child(2n){background-color: #F9FAFC; }
        .edit{background-color: #fff;width: 80px; height: 30px;color: #B69A6A;border: 1px solid #B69A6A;border-radius: 5px;margin-right: 20px;font-size: 14px;}
        .delete{ background: url(../img/images/delete.png); background-size: 100% 100%;width: 20px; height: 20px;border:none; vertical-align: middle;}

        /*添加商品弹框*/
        .close_layer {float:right;cursor: pointer;font-size: 16px; color: #898989; margin-top: -8px;}
        .open_layer {background: rgba(0,0,0,0.2);width: 100%; height: 100%;position: fixed;top: 0;left: 0;z-index: 9999;display: none; }
        .open_layer > div {background: #fff;top: 0; left: 0;right: 0;bottom: 0;width: 650px;border-radius: 10px;padding-bottom: 12px; overflow: hidden;height: 570px;color: #161616; margin: auto; position: absolute;text-align: center;color: #666;font-size: 12px;}
        .open_layer table{text-align:right;}
        .open_layer table td{padding-right:10px;height:40px;}
        .open_layer table input[type=text],select{border:1px solid #EEEEEE;height: 30px;width: 140px;font-size:12px;border-radius: 3px;padding-left:10px;}
        .open_layer .tit{padding:0 0 20px 0;text-align:center;font-size: 12px;}
        .open_layer .tdw{width: 240px;text-align: left;}
        .open_layer .eqw{text-align: left;}
        .open_layer .eqw .imgdepict{width:100%;height:170px;border:1px solid #EEEEEE;}
        .open_layer .eqw .imgdepict input{background: url(../img/images/addimg.jpg);border-radius: 3px;background-size: 100% 100%;width: 60px;width: 60px; height: 60px; margin: 0 0 10px 10px;border: none; text-decoration: none;}
        .open_layer .eqw .imgdepict textarea{width:99%;height: 95px; border: none;outline:none;font-size:12px;font-family:"Microsoft YaHei";}

        .open_layer .editsave{text-align:center;padding-top: 20px;}
        .open_layer .editsave input{width:250px; height: 35px; background-color: #B69B69;font-size: 16px;color: #fff;border-radius: 5px; border: none;}

      
        .nvaL .nav_menu{list-style:none;margin:auto;margin-top:20px;}
        .nvaL .nav_menu a{color:#AFAFAF;text-decoration:none;text-align:center;}
        .nvaL .nav_menu li{padding-top:20px;margin:0;color:#FEFEFE;}
        .nvaL .nav_menu .pitchon a{color:#FEFEFE;}
        .nvaL .nav_menu ul{margin-left:-25px;}

        .nvaL .nav_other {color:#FEFEFE;margin-top:50px;}
        .nvaL .nav_other a{display:block;text-align:center;color:#AFAFAF;font-size:12px; }
