.jfk-calendar {
  &__tools {
    .switch-month {
      &:before, &:after {
        background-color: #fff;
      }
      &.is-disabled {
        &:before, &:after {
          background-color: #bfbfbf;
        }
      }
    }
    .title {
    }
  }
  &__thead {
    &:before {
      background-color: #ebebeb;
    }
  }
}