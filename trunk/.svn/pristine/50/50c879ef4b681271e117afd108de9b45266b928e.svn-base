.jfk-calendar {
  &__tools {
    .switch-month {
      &:before, &:after {
        background-color: #fff;
      }
      &.is-disabled {
        &:before, &:after {
          background-color: #bfbfbf;
        }
      }
    }
    .title {
    }
  }
  &__thead {
    &:before {
      background-color: #ebebeb;
    }
  }
  &__row {
    &.is-disabled {
      .jfk-calendar {
        &__number, &__content {
          color: #ccc;
        }
      }
    }
    &.is-selected {
      .jfk-calendar{
        &__value {
          .date {
            color: #fff;
          }
        }
      }
    }
  }
}