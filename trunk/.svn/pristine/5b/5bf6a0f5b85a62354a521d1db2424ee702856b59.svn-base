@charset "utf-8";
/* CSS Document */
/*公用样式start*/
@font-face {
  font-family: 'iconfont';
  src: url('../font/iconfont.eot');
  src: url('../font/iconfont.eot?#iefix') format('embedded-opentype'),
  url('../font/iconfont.woff') format('woff'),
  url('../font/iconfont.ttf') format('truetype'),
  url('../font/iconfont.svg#iconfont') format('svg');
}
.iconfont{
  font-family:"iconfont" !important;
  font-size:16px;font-style:normal;
  -webkit-font-smoothing: antialiased;
  -webkit-text-stroke-width: 0.2px;
  -moz-osx-font-smoothing: grayscale;
}
html,body{height:100%;width:100%;min-height:504px;position:relative;font-family:"MicrosoftYaHei";}
.color_ffde00{color:#ffde00;}
.clearfix:after{content: "." ;display:block;height:0;clear:both;visibility:hidden;} 
.m_t_5{margin-top:5%;}
.m_t_2{margin-top:2%;}
.m_t_1{margin-top:1%;}
.weight{font-weight:bold;}
.border_bottom,.border_top{position:relative;}
.border_bottom:before{
	content: '';
	position:absolute;
	bottom:0px;
	left:0;
	border-bottom: 1px solid #e5e5e5 !important;
	-webkit-box-sizing:border-box;
	box-sizing: border-box;
	width: 200%;
	height: 0;
	-webkit-transform:scale(0.5);
	transform:scale(0.5);
	-webkit-transform-origin:left bottom;
	transform-origin:left bottom;
}
.border_bottom:after{
	height:0px;bottom:0;
}
.border_top:after{
	content:'';
	position:absolute;
	top:0px;
	left:0;
	border-top:1px solid #e5e5e5;
	-webkit-box-sizing:border-box;
	box-sizing:border-box;
	width:200%;
	height:0;
	-webkit-transform:scale(0.5);
	transform:scale(0.5);
	-webkit-transform-origin:left top;
	transform-origin:left top;
}
.f_l{float:left;}
.f_r{float:right;}
.bg_fff{background:#fff;}
.bg_fc9326{background:#fc9326;}
.bg_fb4947{background:#fb4947}
.bg_4bc1f8{background:#4bc1f8}
.bg_ff9900{background:#ff9900;}
.bg_fc9326{background:#fc9326;}
.bg_e7e7e7{background:#e7e7e7;}
.c_fff{color:#fff;}
.c_000{color:#000;}
.c_989898{color:#989898;}
.c_ff9f0f{color:#ff9f0f;}
.c_8b8b8b{color:#8b8b8b;}
.c_ff950d{color:#ff950d;}
.c_2ac833{color:#2ac833;}
.c_fb4947{color:#fb4947;}
.m_t_3{margin-top:4%;}
.m_b_3{margin-bottom:4%;}
.m_r_2{margin-right:2%;}
.relative{position:relative;}
.absolute{position:absolute;}
.center{text-align:center;}
/*公用样式end*/

/*搜索页面*/
.search{padding:10px;}
.search >div{display:inline-block;}
.in_input{width:83%;border:1px solid #989898;border-radius:50px;padding-left:0.714rem;}
.in_input >input{height:100%;}
.in_input i{font-size:1.214rem;vertical-align:middle;}
.confirm{padding:0 2.3%;}
.con_block >img{width:90px;height:95px;overflow:hidden;margin:auto;margin-right:2%;}
.con_txt{padding-top:15px;}
.con_txt>p{white-space:nowrap;text-overflow:ellipsis;overflow:hidden;}
.c_btn{padding:1px 8px;margin-right:4%;border-radius:3px;}




/*日历页面*/
.title_con{width:92%;margin:auto;color:#000;padding:10px 0;}
.explain{text-align:center;padding:12px 0;}
.explain div{display:inline-block;margin-right:5%;}
.explain div:last-child{margin-right:0%}
.explain div >span{display:inline-block;width:14px;height:14px;border:1px solid #e5e5e5;vertical-align:text-bottom;}

.check{padding-left:3%;}
.on_rows{padding:5px 0;}
.on_rows span:nth-of-type(1){display:inline-block;width:85px;}
.book_btn{width:31%;margin:10px auto;padding:10px 0;color:#fff;border-radius:3px;text-align:center;}

.fixed{position:fixed;top:0px;left:0px;width:100%;height:100%;background:rgba(0,0,0,0.6);display:none;}
.f_con{width:67%;margin:auto;margin-top:10%;border-radius:7px;padding:5% 6%; }
.spot{width:11%;margin:auto;height:5px;border-radius:5px;}
.f_txt{padding:10px 0;}
.f_txt_box{margin:10px 0;}
.f_txt_box >div{margin-bottom:2%;color:#3e3e3e;}
.f_txt_box >div span:nth-of-type(1){margin-right:1%;display:inline-block;width:55px;}
.prompt{color:#989898;padding-left:1.5em;text-indent:-1.5em}
.prompt i{margin-right:2%}
.btn_lists{}
.btn_lists >a,.btn_lists >div{display:inline-block;margin-right:18px;border-radius:2px;padding:3px 8px;}
.btn_lists >a:last-child,.btn_lists >div:last-child{margin-right:0px;}
.btn_lists >a{padding:3px 12px;}
.cancel{border:1px solid #000;}
.determine{border:1px solid #fc9326;}
.close{margin-top:3%;}
.close >i{font-size:30px;}


/*日历*/
table{width:100%;text-align:center;}
tr td div{margin:5px;padding:5px 0;}
tr td div.full{color:#fff;background:#fb4947}
tr td div.no_holet{color:#fff;background:#3d98fb}
tr td div.hook{background: url(../images/hook.png) no-repeat;background-size:65%;background-position:58% 98%;}
.bottom_btn{vertical-align:text-top;margin-left:5px;}
.left_btn{margin-right:10px;}
.right_btn{margin-left:10px;}
.theads >tr{height:30px;}
tbody .titl_btn{height:40px !important;}
.bom_select{position:absolute;bottom:-14px;left:50%;display:none;}
select{font-size:15px;padding-right:20px;background:url(../images/three.png) no-repeat;background-size:26%;background-position:center right;}



/*预订失败  预订成功*/
.heads{padding:3% 0;}
.heads >i{font-size:40px;}
.relevant_t{padding:5px 0;margin-left:3%;}
.relevant_t img{height:14px;width:16px;vertical-align:middle;margin-right:2%;}
.relevant_con{margin:3%;}
.r_c_box{display:inline-block;width:48.3%;margin-right:2%;}
.r_c_box:nth-of-type(2n){margin-right:0px;}
.box_img{width:100%;height:auto;}
.box_img img{width:100%;height:100%;}
.box_txt2 i{margin:0 5%;font-style:normal;}




