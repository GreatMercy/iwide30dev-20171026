:root{
  --base-font-size: 16;
  --base-font-family: 'PingFangSC-Light',Arial,Helvetica,sans-serif;
  --size-queue: 16, 18, 20, 22, 24, 26, 28, 30, 32, 34, 36, 38, 40, 44, 46, 48, 50, 54, 60, 65, 68, 80, 120;
  --border-radius-big: 7;
  --border-radius-small: 4;
  --direction: (t, r, b, l), (top, right, bottom, left);
  --price-tag-fill-colors: #6069ad, #6069ad, #7462ad, #9eb169, #b5699e;
  /*表单项*/
  --form-item-line-height: 64;
  --form-item-height: 100;
  --form-item-label-width: 200;
}