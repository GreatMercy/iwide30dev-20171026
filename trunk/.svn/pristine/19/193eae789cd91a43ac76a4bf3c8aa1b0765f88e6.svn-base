.jfk-pages__orderDetail{
    .arrow{
        &:before,&:after{
            background-color: #808080;
        }
    }
    .orderDetail__giftcontent{
        .giftcontent_title{
            color: #808080;
            &:before,&:after{
                background-color: #ebebeb;
            }
        }
        .giftcontent_fortune{
            box-shadow: 3px 3px 21px 3px rgba(102,102,102,0.15);
            .forturn__name{
                color: #808080;
                padding-left: px2rem(90);
                i{
                    color: #b2945e;
                    text-shadow: none;
                }
            }
            .forturn__value{
                color: #b2945e;
            }
        }

    }
}